{
  "cli": {
    "version": ">=16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "API_BASE_URL": "https://clinix-backend-te9n.onrender.com/api"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "API_BASE_URL": "https://clinix-backend-te9n.onrender.com/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "API_BASE_URL": "https://clinix-backend-te9n.onrender.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
